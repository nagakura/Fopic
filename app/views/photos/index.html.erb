<h1>Listing photos</h1>

<table id="cal">
  <tr>
    <th class="day">Mon</th>
    <th class="day">Tue</th>
    <th class="day">Wed</th>
    <th class="day">Thu</th>
    <th class="day">Fri</th>
    <th class="day">Sat</th>
    <th class="day">Sun</th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td>
    <%= photo.created_at %>
    <br/>
    <%= link_to image_tag(picture_photo_path(photo), :height => '64'), photo %>
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
